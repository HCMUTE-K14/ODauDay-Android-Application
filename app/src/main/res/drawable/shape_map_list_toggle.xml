<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <corners android:radius="100dp"/>
      <padding
        android:bottom="2dp"
        android:left="1dp"
        android:right="1dp"
        android:top="2dp"/>
      <gradient
        android:angle="270.0"
        android:endColor="#197e8594"
        android:startColor="#05d0d3d9"/>
    </shape>
  </item>
  <item>
    <shape android:shape="rectangle">
      <solid android:color="@color/white"/>
      <corners android:radius="100dp"/>
      <padding
        android:bottom="2dp"
        android:left="15dp"
        android:right="15dp"
        android:top="2dp"/>
    </shape>
  </item>
</layer-list>
